# FinFind Environment Configuration
# Copy this file to .env and fill in your values

# ═══════════════════════════════════════════════════════════════
# QDRANT CLOUD CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# Cluster URL (from Qdrant Cloud dashboard)
QDRANT_URL=https://your-cluster-id.us-east-1.aws.cloud.qdrant.io

# API Key (from Qdrant Cloud → API Keys)
QDRANT_API_KEY=your_qdrant_api_key_here

# Connection Settings
QDRANT_TIMEOUT=30
QDRANT_PREFER_GRPC=true

# ═══════════════════════════════════════════════════════════════
# GROQ API CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# API Key (from console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# Model Selection
GROQ_MODEL=llama-3.3-70b-versatile

# Rate Limiting
GROQ_RATE_LIMIT_RPM=30
GROQ_RATE_LIMIT_TPM=6000

# ═══════════════════════════════════════════════════════════════
# COLLECTION NAMES
# ═══════════════════════════════════════════════════════════════

QDRANT_PRODUCTS_COLLECTION=products
QDRANT_USER_PROFILES_COLLECTION=user_profiles
QDRANT_REVIEWS_COLLECTION=reviews
QDRANT_INTERACTIONS_COLLECTION=user_interactions

# ═══════════════════════════════════════════════════════════════
# EMBEDDING CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# Text Embedding Model
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
TEXT_EMBEDDING_DIM=384

# Image Embedding Model (for multi-modal)
IMAGE_EMBEDDING_MODEL=openai/clip-vit-base-patch32
IMAGE_EMBEDDING_DIM=512

# Batch Processing
EMBEDDING_BATCH_SIZE=64

# ═══════════════════════════════════════════════════════════════
# APPLICATION SETTINGS
# ═══════════════════════════════════════════════════════════════

# URLs
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000

# Debug Mode
DEBUG=true
LOG_LEVEL=INFO

# ═══════════════════════════════════════════════════════════════
# SEARCH CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# Default search limits
DEFAULT_SEARCH_LIMIT=20
MAX_SEARCH_LIMIT=100

# MMR (Maximal Marginal Relevance) settings
MMR_LAMBDA=0.7
MMR_FETCH_MULTIPLIER=3

# HNSW search settings
HNSW_EF=128

# ═══════════════════════════════════════════════════════════════
# DATA GENERATION (Optional - for development)
# ═══════════════════════════════════════════════════════════════

# Generation counts
GEN_NUM_PRODUCTS=500
GEN_NUM_USERS=100
GEN_NUM_REVIEWS=1200
GEN_NUM_INTERACTIONS=2500

# Random seed for reproducibility
GEN_RANDOM_SEED=42
